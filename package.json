{
  "name": "irwin-framework",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Peter Pham",
  "license": "MIT",
  "scripts": {
    "build": "yarn build-client && npx tsc && node dist/index.js",
    "serverless": "yarn build-client && npx tsc && npx serverless offline --noPrependStageInUrl",
    "start": "yarn build-client && node dist/index.js",
    "dev": "nodemon -e js,ts,pug,vue index.ts",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:seed": "npx ts-node-dev models/prisma/seed.ts",
    "build-client": "npx browserify -t vueify -e app/assets/javascripts/main.ts -o app/assets/javascripts/generated/bundle.js",
    "git": "sh .vscode/git.sh",
    "routes": "ts-node commands/routes.ts"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/paypal-server-sdk": "^0.6.0",
    "@prisma/client": "^5.19.1",
    "axios": "^1.7.3",
    "bcrypt": "^5.1.1",
    "body-parser": "1.20.2",
    "bootstrap": "5.3.2",
    "browserify": "17.0.0",
    "child_process": "1.0.2",
    "cookie-parser": "1.4.6",
    "dayjs": "^1.11.12",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-flash": "0.0.2",
    "express-session": "1.17.1",
    "font-awesome": "4.7.0",
    "googleapis": "^140.0.1",
    "jquery": "3.7.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "4.17.5",
    "method-override": "3.0.0",
    "multer": "1.4.5-lts.1",
    "node-sass": "9.0.0",
    "nodemailer": "6.9.14",
    "pug": "3.0.2",
    "serverless": "3.36.0",
    "serverless-http": "3.2.0",
    "serverless-offline": "13.2.1",
    "sqlite3": "5.1.7",
    "tsconfig-paths": "4.2.0",
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "1.4.6",
    "@types/express": "^4.17.13",
    "@types/express-flash": "0.0.5",
    "@types/express-session": "1.17.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "4.17.5",
    "@types/method-override": "0.0.35",
    "@types/multer": "1.4.11",
    "@types/node": "22.1.0",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "6.4.15",
    "@types/paypal__checkout-server-sdk": "^1.0.8",
    "@types/validator": "10.11.3",
    "nodemon": "3.0.1",
    "prisma": "^5.19.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prisma": {
    "schema": "./models/prisma/schema.prisma"
  }
}
